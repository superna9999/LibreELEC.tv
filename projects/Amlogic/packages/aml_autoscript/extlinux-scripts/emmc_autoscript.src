if test ${bls} = android; then setenv bls "libreelec"; saveenv; run storeboot; fi;
setenv env_addr "0x10400000"
setenv kernel_loadaddr "0x11000000"
setenv dtb_mem_addr "0x1000000"
setenv bootargs ddr_size=${ddr_size} ${bootargs}
setenv boot_start booti ${kernel_loadaddr} - ${dtb_mem_addr}
if fatload mmc 1 ${kernel_loadaddr} Image; then if fatload mmc 1 ${env_addr} uEnv.ini; then env import -t ${env_addr} ${filesize}; setenv bootargs ${boot_env} ${bootargs}; fi; if fatload mmc 1 ${dtb_mem_addr} ${dtb_name}; then run boot_start;fi;fi;
